<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
    <system.serviceModel>
        <bindings>
            <customBinding>
                <binding name="InvoiceWSPortBinding">
                    <textMessageEncoding messageVersion="Soap12" />
                    <httpsTransport />
                </binding>
            </customBinding>
        </bindings>
        <client>
            <endpoint address="https://servis.univeradonusum.com/InvoiceService/InvoiceWS"
                binding="customBinding" bindingConfiguration="InvoiceWSPortBinding"
                contract="InvoiceWS.InvoiceWS" name="InvoiceWSPort" />
        </client>
    </system.serviceModel>
</configuration>